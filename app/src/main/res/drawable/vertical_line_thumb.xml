<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape android:shape="rectangle">

            <solid android:color="@color/md_theme_light_background" />
        </shape>
    </item>


    <item
        android:left="6dp"
        android:right="6dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/md_theme_dark_background" />
            <solid android:color="#356083" />
            <size
                android:width="4dp"
                android:height="44dp" />
            <corners android:radius="3dp" />

        </shape>
    </item>
</layer-list>