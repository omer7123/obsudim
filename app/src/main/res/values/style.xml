<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TitleFragment" parent="TextAppearance.Material3.ActionBar.Title">
        <item name="android:titleTextColor">@color/md_theme_light_onSurface</item>
        <item name="fontFamily">@font/cygre_semi_bold</item>
        <item name="android:textSize">32sp</item>
    </style>

    <style name="BodyM">
        <item name="fontFamily">@font/inter_medium</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="BodyL">
        <item name="fontFamily">@font/inter_medium</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="BodyMBold">
        <item name="fontFamily">@font/inter_bold</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TitleXS">
        <item name="fontFamily">@font/inter_bold</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="BodyXLBold">
        <item name="android:textSize">18sp</item>
